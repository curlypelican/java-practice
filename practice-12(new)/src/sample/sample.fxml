<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight = "-Infinity" maxWidth = "-Infinity" minHeight = "-Infinity" minWidth = "-Infinity"
            prefHeight = "195.0" prefWidth = "270.0" xmlns = "http://javafx.com/javafx/17"
            xmlns:fx = "http://javafx.com/fxml/1" fx:controller = "sample.Controller">
<children>
    <Button fx:id = "button1" layoutX = "30.0" layoutY = "80.0" mnemonicParsing = "false" text = "+" />
    <Button fx:id = "button2" layoutX = "68.0" layoutY = "80.0" mnemonicParsing = "false" text = "-" />
    <Button fx:id = "button3" layoutX = "100.0" layoutY = "80.0" mnemonicParsing = "false" text = "*" />
    <Button fx:id = "button4" layoutX = "133.0" layoutY = "80.0" mnemonicParsing = "false" text = "/" />
    <Button fx:id = "button5" layoutX = "165.0" layoutY = "80.0" mnemonicParsing = "false" text = "=" />
    <TextField fx:id = "input" layoutX = "30.0" layoutY = "25.0" prefHeight = "30.0" prefWidth = "160.0"
               promptText = "Введите число" />
    <TextField fx:id = "output" layoutX = "30.0" layoutY = "145.0" prefHeight = "30.0" prefWidth = "160.0"
               promptText = "Ответ"/>
</children>
</AnchorPane>




